  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "def words_in_texts_toDF(data, words):\n",
    "    words_matrix = words_in_texts(words, data['email'])\n",
    "    df = pd.DataFrame(words_matrix, columns = words)\n",
    "    return data.reset_index().join(df)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "def add_punc_col(data, col, symbol):\n",
    "    data['num of ' + symbol + ' in ' + col] = data[col].str.count('\\\\' + symbol)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "def create_x_data(data, is_train):\n",
    "    X_train = words_in_texts_toDF(data, some_words)\n",
    "    add_reply_col(X_train, 'subject')\n",
    "    add_fw_col(X_train, 'subject')\n",
    "    add_punc_col(X_train, 'email', '!')\n",
    "    add_punc_col(X_train, 'subject', '!')\n",
    "    add_punc_col(X_train, 'email', '?')\n",
    "    add_punc_col(X_train, 'subject', '?')\n",
    "    X_train.fillna(0, inplace = True)\n",
    "    X_train = X_train.drop(columns=['id', 'subject', 'email'])\n",
    "    if is_train:\n",
    "        X_train = X_train.drop(columns=['spam'])\n",
    "    return X_train\n",
    "\n",
    "def create_y_data(data):\n",
    "    return data['spam']"
   ]
  },
  {
